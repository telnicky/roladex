<h1>Listing locations</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>County code</th>
      <th>Phone</th>
      <th>Employees</th>
      <th>Employee code</th>
      <th>Naics</th>
      <th>Ownership</th>
      <th>Company</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @locations.each do |location| %>
      <tr>
        <td><%= location.address %></td>
        <td><%= location.city %></td>
        <td><%= location.state %></td>
        <td><%= location.zip %></td>
        <td><%= location.county_code %></td>
        <td><%= location.phone %></td>
        <td><%= location.employees %></td>
        <td><%= location.employee_code %></td>
        <td><%= location.naics %></td>
        <td><%= location.ownership %></td>
        <td><%= location.company_id %></td>
        <td><%= link_to 'Show', location %></td>
        <td><%= link_to 'Edit', edit_location_path(location) %></td>
        <td><%= link_to 'Destroy', location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location', new_location_path %>
